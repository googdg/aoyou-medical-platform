    "socket.io": "^4.7.4",
    "multer": "^1.4.5-lts.1",
    "bull": "^4.12.2",
    "ioredis": "^5.3.2",
    "sqlite3": "^5.1.6",
    "pg": "^8.11.3",
    "winston": "^3.11.0",
    "joi": "^17.11.0",
    "uuid": "^9.0.1",
    "mime-types": "^2.1.35",
    "fluent-ffmpeg": "^2.1.2",
